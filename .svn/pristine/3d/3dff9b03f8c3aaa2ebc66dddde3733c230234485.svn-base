package api.stonedt.com.entity;

public class UserEntity {
	
	public UserEntity() {
		super();
		// TODO Auto-generated constructor stub
	}
	//序号ID
	private int id;
	//用户账号
	private String account;
	//用户密码
	private String password;
	//真实姓名
	private String realname;
	//联系方式
	private String telephone;
	//角色 1代表主账户、2代表子账户
	private int role;
	//医院id
	private int hospitalid;
	//有参
	public UserEntity(int id, String account, String password, String realname, String telephone, int role,
			int hospitalid) {
		super();
		this.id = id;
		this.account = account;
		this.password = password;
		this.realname = realname;
		this.telephone = telephone;
		this.role = role;
		this.hospitalid = hospitalid;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getAccount() {
		return account;
	}
	public void setAccount(String account) {
		this.account = account;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getRealname() {
		return realname;
	}
	public void setRealname(String realname) {
		this.realname = realname;
	}
	public String getTelephone() {
		return telephone;
	}
	public void setTelephone(String telephone) {
		this.telephone = telephone;
	}
	public int getRole() {
		return role;
	}
	public void setRole(int role) {
		this.role = role;
	}
	public int getHospitalid() {
		return hospitalid;
	}
	public void setHospitalid(int hospitalid) {
		this.hospitalid = hospitalid;
	}
	//toString 方法
	@Override
	public String toString() {
		return "UserEntity [id=" + id + ", account=" + account + ", password=" + password + ", realname=" + realname
				+ ", telephone=" + telephone + ", role=" + role + ", hospitalid=" + hospitalid + "]";
	}
	
	
	

}
