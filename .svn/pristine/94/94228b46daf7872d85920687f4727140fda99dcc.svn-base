<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%
String path = request.getContextPath();
String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";
%>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<base href="<%=basePath%>">

<title>您的浏览器不支持</title>


</head>

<body style="background-color: #3CBBDC">
	<center style="padding-top: 200px">
		<img style="cursor: pointer;"
			src="${pageContext.request.contextPath }/static/img/browser.png"
			onclick="location.href='http://download.firefox.com.cn/releases-sha2/stub/official/zh-CN/Firefox-latest.exe'">
	</center>
</body>
</html>
