package api.stonedt.com.entity;

import java.util.Date;

public class Hospitalnews {
	//   无参
        public Hospitalnews(){
        	super();
        }
        //id
        private int id;
        //评论用户名
        private String identification;
        //原始html页面
        private String content;
        //文章纯文本
        private String text;
        //原始url
        private String weburl;
        //日期
        private Date date;
        //标题
        private String title;
        //关键词
        private String keyword;
        //医院id
        private int hospitalid;
        //1代表正面、2代表负面、3代表中性
        private int emotion;
        //类别
        private int kind;
        //原始网站
        private String originweb;
        //概要
        private String abstractcontent;
        //其他医院id
        private int otherhospitalid;
        //1代表新闻、2代表用户评论
        private int comment;
        //相关医生的id
        private int doctorid;
        //医生相关新闻的类型，1代表网络相关、2代表学术著作
        private int type;
		public int getId() {
			return id;
		}
		public void setId(int id) {
			this.id = id;
		}
		public String getIdentification() {
			return identification;
		}
		public void setIdentification(String identification) {
			this.identification = identification;
		}
		public String getContent() {
			return content;
		}
		public void setContent(String content) {
			this.content = content;
		}
		public String getText() {
			return text;
		}
		public void setText(String text) {
			this.text = text;
		}
		public String getWeburl() {
			return weburl;
		}
		public void setWeburl(String weburl) {
			this.weburl = weburl;
		}
		public Date getDate() {
			return date;
		}
		public void setDate(Date date) {
			this.date = date;
		}
		public String getTitle() {
			return title;
		}
		public void setTitle(String title) {
			this.title = title;
		}
		public String getKeyword() {
			return keyword;
		}
		public void setKeyword(String keyword) {
			this.keyword = keyword;
		}
		public int getHospitalid() {
			return hospitalid;
		}
		public void setHospitalid(int hospitalid) {
			this.hospitalid = hospitalid;
		}
		public int getEmotion() {
			return emotion;
		}
		public void setEmotion(int emotion) {
			this.emotion = emotion;
		}
		public int getKind() {
			return kind;
		}
		public void setKind(int kind) {
			this.kind = kind;
		}
		public String getOriginweb() {
			return originweb;
		}
		public void setOriginweb(String originweb) {
			this.originweb = originweb;
		}
		public String getAbstractcontent() {
			return abstractcontent;
		}
		public void setAbstractcontent(String abstractcontent) {
			this.abstractcontent = abstractcontent;
		}
		public int getOtherhospitalid() {
			return otherhospitalid;
		}
		public void setOtherhospitalid(int otherhospitalid) {
			this.otherhospitalid = otherhospitalid;
		}
		public int getComment() {
			return comment;
		}
		public void setComment(int comment) {
			this.comment = comment;
		}
		public int getDoctorid() {
			return doctorid;
		}
		public void setDoctorid(int doctorid) {
			this.doctorid = doctorid;
		}
		public int getType() {
			return type;
		}
		public void setType(int type) {
			this.type = type;
		}
        public Hospitalnews(int id,String identification,String content,
        		String text,String weburl,Date date,String title,String keyword, int hospitalid,
        		int emotion,int kind,String originweb,String abstractcontent,int otherhospitalid,
        		int comment,int doctorid,int type){
        	this.id=id;this.identification=identification;this.content=content;
        	this.text=text;this.weburl=weburl;this.date=date;this.title=title;this.keyword=keyword;this.hospitalid=hospitalid;
        	this.emotion=emotion;this.kind=kind;this.originweb=originweb;this.abstractcontent=abstractcontent;this.otherhospitalid=otherhospitalid;
        	this.comment = comment;this.doctorid=doctorid;this.type=type;
        };
        
}
